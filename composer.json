{
    "name": "notchafrica/func",
    "description": "This is my package func",
    "keywords": [
        "notchafrica",
        "laravel",
        "func"
    ],
    "homepage": "https://github.com/notchafrica/func",
    "license": "MIT",
    "authors": [
        {
            "name": "Chapdel KAMGA",
            "email": "drop@chapdel.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "algolia/scout-extended": "^3.0",
        "chapdel/lawyer": "^1.0",
        "cybercog/laravel-ban": "^4.8",
        "cyrildewit/eloquent-viewable": "^7.0",
        "geoip2/geoip2": "^2.13",
        "glorand/laravel-model-settings": "^6.0",
        "illuminate/contracts": "*",
        "jamesmills/laravel-timezone": "^1.13",
        "laracraft-tech/laravel-date-scopes": "^1.1",
        "league/flysystem-aws-s3-v3": "^3.16",
        "monarobase/country-list": "^3.4",
        "predis/predis": "^2.2",
        "propaganistas/laravel-phone": "^5.0",
        "rennokki/laravel-eloquent-query-cache": "^3.4",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-deleted-models": "^1.0",
        "spatie/laravel-model-status": "^1.14",
        "spatie/laravel-package-tools": "^1.14.0",
        "spatie/laravel-webhook-server": "^3.6",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "NotchAfrica\\Func\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotchAfrica\\Func\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-func --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotchAfrica\\Func\\FuncServiceProvider"
            ],
            "aliases": {
                "Func": "NotchAfrica\\Func\\Facades\\Func"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
